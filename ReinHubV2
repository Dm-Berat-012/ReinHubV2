-- FAKE DROP and X RAY

-- FAKE DROP

local Tab = Window:NewTab("Fake Drop - X-Ray")

local Section = Tab:NewSection("Fake Drop")

Section:NewButton("Fake Drop", "Yere Sahte Eşya Atma", function()

    local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.RightHand:Destroy()

print("Dropped - Atıldı")

end)  

Section:NewButton("Reset", "Karakteri Resetleme", function()

local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.Head:Destroy()

end)

-- X RAY 

local Section = Tab:NewSection("X-Ray")  Section:NewButton("X-Ray", "Bedava X-Ray Kullan!", function()

    game.Players.localPlayer.XRay.Value = true

end)

